{
  "env": {
    "development": {
      "presets": ["next/babel"],
      "plugins": [
        ["styled-components", { "ssr": true }],
        [
          "module-resolver",
          {
            "root": ["./"],
            "alias": {
              "@common": "./src/common",
              "@components": "./src/components",
              "@store": "./src/store",
              "@styles": "./src/styles"
            }
          }
        ]
      ]
    },
    "production": {
      "presets": ["next/babel"],
      "plugins": [
        ["styled-components", { "ssr": true }],
        [
          "module-resolver",
          {
            "root": ["./"],
            "alias": {
              "@common": "./src/common",
              "@components": "./src/components",
              "@store": "./src/store",
              "@styles": "./src/styles"
            }
          }
        ]
      ]
    }
  }
}
